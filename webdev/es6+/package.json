    "babel-core": "^6.0.20",
    "babel-preset-latest": "^6.24.1",
    "browserify": "^16.2.3",
    "minify": "^3.0.5",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "assert": "^1.4.1",
    "express": "^4.16.3",
    "request": "^2.88.0"
  }
}

